{
  "name": "dbt/blame",
  "license": "MIT",
  "require": {
    "php" : ">=7.1.3",
    "illuminate/support": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "^3.7",
    "dbt/phpunit-runner": "^0.1.2",
    "vimeo/psalm": "^3.2"
  },
"autoload": {
    "psr-4": {
        "Dbt\\Blame\\": "src/"
    }
},
  "autoload-dev": {
    "psr-4": {
      "Dbt\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "php ./vendor/dbt/phpunit-runner/phpunit.php --no-coverage",
    "coverage": "phpunit --coverage-html coverage",
    "stat": "psalm"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dbt\\Blame\\Provider"
      ]
    }
  }
}
